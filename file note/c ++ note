//lambda function 
    [] (int x,int y) {cout<< x+y;} (4 ,5)      ; // in ra 9
    [] : capture list // list of external variables that the lambda function can access
    (int x,int y) : parameter list
    {cout<< x+y;} : body
    (4 ,5) : argument list

    //  int x =5 ;[x] (int y) {cout<< x+y;} (5)      ; // in ra 10
